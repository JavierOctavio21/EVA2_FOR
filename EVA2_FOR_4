public class Principal {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int i;
        for (i = 100; i >= 1; i--) {
            System.out.println(i);
        }
        int j;
        for (j = 100; j >= 1; j--) {
            System.out.print(j +" - ");
        }
    }
}
